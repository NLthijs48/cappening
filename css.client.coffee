Dom.css
    '.bar-button':
        height: "100%"
        width: "25%"
        float: "right"
        backgroundColor: "#666"
        lineHeight: "50px"	
    '.bar-button::after':
        content: '""'
        display: 'block'
        position: 'absolute'
        right: 0
        top: 0
        width: '1px'
        height: '40px'
        backgroundColor: '#DDD'
        margin: 'auto 0px'
    '.mapbox-logo':
        display: "none"